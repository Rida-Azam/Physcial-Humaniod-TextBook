      </geometry>
      <material name="red"/>
    </visual>
    <collision>
      <origin xyz="0 0 0.15"/>
      <geometry>
        <box size="0.2 0.15 0.3"/>
      </geometry>
    </collision>
  </link>

  <joint name="pelvis_to_torso" type="fixed">
    <parent link="pelvis"/>
    <child link="torso"/>
    <origin xyz="0 0 0.15"/>
  </joint>

  <!-- Head -->
